<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Main Menu -->
    <menuitem id="menu_docker_root"
              name="Docker"
              sequence="90"
              web_icon="j_docker,static/description/icon.png"/>
    
    <!-- Dashboard Menu -->
    <menuitem id="menu_docker_dashboard"
              name="Dashboard"
              parent="menu_docker_root"
              action="action_docker_server"
              sequence="10"/>
    
    <!-- Servers Menu -->
    <menuitem id="menu_docker_servers"
              name="Servers"
              parent="menu_docker_root"
              sequence="20"/>
              
    <menuitem id="menu_docker_server_list"
              name="Docker Servers"
              parent="menu_docker_servers"
              action="action_docker_server"
              sequence="10"/>
    
    <!-- Containers Menu -->
    <menuitem id="menu_docker_containers"
              name="Containers"
              parent="menu_docker_root"
              sequence="30"/>
              
    <menuitem id="menu_docker_container_list"
              name="All Containers"
              parent="menu_docker_containers"
              action="action_docker_container"
              sequence="10"/>
    
    <menuitem id="menu_docker_container_running"
              name="Running Containers"
              parent="menu_docker_containers"
              action="action_docker_container_running"
              sequence="20"/>
    
    <!-- Images Menu -->
    <menuitem id="menu_docker_images"
              name="Images"
              parent="menu_docker_root"
              sequence="40"/>
              
    <menuitem id="menu_docker_image_list"
              name="All Images"
              parent="menu_docker_images"
              action="action_docker_image"
              sequence="10"/>
    
    <menuitem id="menu_docker_image_orphaned"
              name="Dangling Images"
              parent="menu_docker_images"
              action="action_docker_image_dangling"
              sequence="20"/>
    
    <!-- Networks Menu -->
    <menuitem id="menu_docker_networks"
              name="Networks"
              parent="menu_docker_root"
              sequence="50"/>
              
    <menuitem id="menu_docker_network_list"
              name="All Networks"
              parent="menu_docker_networks"
              action="action_docker_network"
              sequence="10"/>
    
    <!-- Volumes Menu -->
    <menuitem id="menu_docker_volumes"
              name="Volumes"
              parent="menu_docker_root"
              sequence="60"/>
              
    <menuitem id="menu_docker_volume_list"
              name="All Volumes"
              parent="menu_docker_volumes"
              action="action_docker_volume"
              sequence="10"/>
    
    <!-- Tasks Menu -->
    <menuitem id="menu_docker_tasks"
              name="Tasks"
              parent="menu_docker_root"
              sequence="70"/>
              
    <menuitem id="menu_docker_task_list"
              name="All Tasks"
              parent="menu_docker_tasks"
              action="action_docker_task"
              sequence="10"/>
    
    <menuitem id="menu_docker_task_scheduled"
              name="Scheduled Tasks"
              parent="menu_docker_tasks"
              action="action_docker_task_scheduled"
              sequence="20"/>
    
    <!-- Logs Menu -->
    <menuitem id="menu_docker_logs"
              name="Logs"
              parent="menu_docker_root"
              sequence="80"/>
              
    <menuitem id="menu_docker_logs_list"
              name="All Logs"
              parent="menu_docker_logs"
              action="action_docker_logs"
              sequence="10"/>
    
    <!-- Configuration Menu -->
    <menuitem id="menu_docker_configuration"
              name="Configuration"
              parent="menu_docker_root"
              sequence="100"
              groups="j_docker.group_docker_manager"/>
</odoo>